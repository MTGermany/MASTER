  "GKTtype2b-OCT.x23000" u ($2-60):4 t "1 km downstream " w l ls 3,\
  "GKTtype2b-OCT.x25000" u ($2-60):4 t "3 km downstream " w l ls 3,\
  0, 110*t t "begin/end of obstruction" w l ls 10,\
  30, 110*t t "" w l ls 10

