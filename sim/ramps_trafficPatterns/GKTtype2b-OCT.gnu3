set term post eps enhanced color solid "Helvetica" 28
set out "GKTtype2b-OCT_fund.eps"
print "plotting GKTtype2b-OCT_fund.eps ..."
set size 1,1
set xlabel "{/Symbol r}(vehicles/km)" 0,0.5
set xrange [0:130]
set ylabel "Q (vehicles/h)" 1,0
set auto y
set nokey
set nogrid

rhoc1=20.
rhoc2=24.
rhocconv=39.
rhoc3=50.
rhoc4=55.

Qc1=2000.
Qc2=2150.
Qcconv=1800.
Qc3=1500.
Qc4=1420.

set ytics 500
set param

plot[t=0:1]\
 "GKTtype2b-OCT.tab" u 1:4 t "Q_e" w l ls 1,\
 rhoc1, t*Qc1 t "Limit {/Symbol r}_{c1}" w l ls 2,\
 rhoc2, t*Qc2 t "Limit {/Symbol r}_{c2}" w l ls 3,\
 rhocconv, t*Qcconv t "Limit {/Symbol r}_{conv}" w l lt 0,\
 rhoc3, t*Qc3 t "Limit {/Symbol r}_{c3}" w l ls 3,\
 rhoc4, t*Qc4 t "Limit {/Symbol r}_{c4}" w l ls 2


quit

